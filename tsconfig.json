{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "rootDir": ".",
    "typeRoots": [
      "src/types",
      "node_modules/@types"
    ],
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "twin.d.ts",
    ".next/types/**/*.ts",
    "app/auth/signup/page.jsx",
    "contexts/AuthContext.jsx"
, "components/dashboard/RadialChart.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
